// Place your settings in this file to overwrite default and user settings.
{
    "python.pythonPath": "venv\\Scripts\\python.exe",
    "python.linting.pylintArgs": ["--load-plugins", "pylint_flask pylint-sqlalchemy"]
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*.pyc": {"when": "$(basename).py"}, 
        "**/__pycache__": true
    }
}
